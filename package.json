    "@testing-library/react": "^8.0.6",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "coveralls": "^3.0.5",
    "css-loader": "^3.0.0",
    "eslint": "^6.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.1",
